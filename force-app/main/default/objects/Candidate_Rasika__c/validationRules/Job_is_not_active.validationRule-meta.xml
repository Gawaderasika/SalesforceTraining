<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Job_is_not_active</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
NOT( Job_Rasika__r.Active__c   =  True),
 $Setup.VRS__c.JobIsNotActive__c 
)</errorConditionFormula>
