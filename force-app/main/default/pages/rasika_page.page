<apex:page controller="rasika_class" >
    
    <head>      
    	<apex:includeScript value="{!URLFOR($Resource.angularMin)}"/>
    <apex:includeScript value="{!URLFOR($Resource.jqueryMin)}"/>
    	<apex:stylesheet value="{!URLFOR($Resource.rasika_css)}"/>
    	<apex:includeScript value="{!URLFOR($Resource.rasika_app)}"/>
    	<apex:includeScript value="{!URLFOR($Resource.rasika_factory)}"/>
    	<apex:includeScript value="{!URLFOR($Resource.rasika_directive)}"/>
    	<apex:includeScript value="{!URLFOR($Resource.myJSVendor)}"/>
    </head>
    
    <html>
        <body ng-app = "rasika_app"  ng-controller="rasika_controller">
            <button ng-click = "selectButton('Account')">
                Account
            </button>
            <button ng-click = "selectButton('Contact')">
                Contact
            </button>
            <button ng-click = "selectButton('Job_Rasika__c')">
                Job
            </button>
            <button ng-click = "selectButton('Candidate_Rasika__c')">
                Candidate
            </button>
            <rasika-directive fields= 'fields' records = 'result' 
                     s-object-name = 'contact' 
                     delete-action = 'deleteRecord(sObjectName, Id, onSuccess,onError)' >
            </rasika-directive>
        </body>
    </html>
</apex:page>