<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Expected_Salary_field_is_missing</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
ISBLANK( Expected_Salary__c )
,
$Setup.VRS__c.Expiry_date_rule__c
)</errorConditionFormula>
    <errorDisplayField>addError__c</errorDisplayField>
    <errorMessage>Validation error: Expected Salary field is missing</errorMessage>
</ValidationRule>
