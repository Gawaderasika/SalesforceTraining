<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Application_Date</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
 ISCHANGED( Application_Date__c ),
 $Setup.VRS__c.Application_Date_rule__c  
)</errorConditionFormula>
